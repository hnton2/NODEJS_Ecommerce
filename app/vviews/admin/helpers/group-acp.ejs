<% 
    groupACPHelper = (linkPrefix, group_acp, id) => {
        let xhtmlStatus = '';
        let link       = linkPrefix + 'change-group-acp/' + id + '/' + group_acp ;
        if(group_acp === "yes") {
            xhtmlStatus = `<span id="btn-group-acp-${id}" class="my-btn-state rounded-circle btn btn-sm btn-success"><i class="fas fa-check"></i></span>`;
        }else if (group_acp === "no") {
            xhtmlStatus = `<span id="btn-group-acp-${id}" class="my-btn-state rounded-circle btn btn-sm btn-default"><i class="fas fa-check"></span>`;
        }

        return `<a id="abcdefgh" class="group-acp-${id}" href="javascript:void(0)" onclick="changeGroupAcp('${link}')">${xhtmlStatus}</a>`;
    }
%>