<%
    const linkSubmit = systemConfig.prefixShop + '/contact/save';
%>
<div class="ps-contact ps-section pb-80"> 
  <div class="ps-container">
    <div class="row">
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 ">
        <div class="ps-section__header mb-50">
          <h2 class="ps-section__title" data-mask="Contact"> Get in touch</h2>
          <% include ./../../elements/notify %>
          <form class="ps-contact__form" name="contact-form" action="<%= linkSubmit %>" method="POST">
            <div class="row">   
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
                <div class="form-group">
                  <label>Name <sub>*</sub></label>
                  <input class="form-control" id="name-input" name='name' type="text" placeholder="Name">
                  <span data-alertid="example"></span>
                </div>
              </div> 
              <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 ">
                <div class="form-group">
                  <label>Phone <sub>*</sub></label>
                  <input class="form-control" id="phone-input" name='phone' type="text" placeholder="Phone">
                  <span data-alertid="example"></span>
                </div>
              </div>
              <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12 ">
                <div class="form-group">
                  <label>Email <sub>*</sub></label>
                  <input class="form-control" id="email-input" name='email' type="email" placeholder="E-mail">
                  <span data-alertid="example"></span>
                </div>
              </div>
              <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 ">
                <div class="form-group mb-25">
                  <label>Your Message <sub></sub></label>
                  <textarea class="form-control" rows="6"></textarea>
                </div>
                <div class="form-group">
                  <button class="ps-btn">Send Message<i class="ps-icon-next"></i></button>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
      <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="padding-top: 20px;">
        <div class="mapouter">
          <div class="gmap_canvas">
            <iframe width="700" height="400" id="gmap_canvas" src="https://maps.google.com/maps?q=University%20of%20information%20technology&t=&z=15&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
            </iframe>
            <a href="https://123movies-to.org"></a>
            <br>
            <style>.mapouter{position:relative;text-align:right;height:400px;width:700px;}</style>
            <a href="https://www.embedgooglemap.net">google maps widget html</a>
            <style>.gmap_canvas {overflow:hidden;background:none!important;height:400px;width:700px;}</style>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>